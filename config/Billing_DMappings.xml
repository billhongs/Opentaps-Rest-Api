<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">


    <configuration>
        <stop-on-errors>true</stop-on-errors>
    </configuration>


    <mapping>
        <class-a>org.opentaps.module.ws.rest.domain.billing.InvoiceBean</class-a>
        <class-b>org.opentaps.domain.billing.invoice.Invoice</class-b>
        <field>
            <a is-accessible="true">invoiceItems</a>
            <b get-method="getInvoiceItems" set-method="setInvoiceItems">invoiceItems</b>
        </field>
    </mapping>



</mappings>